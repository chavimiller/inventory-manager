<!DOCTYPE html>
<html>
  <head>
    <title>Categories</title>
  </head>
  <body>
    <h1>All Categories</h1>

    <a href="/categories/new">Add New Category</a>

    <ul>
      <% categories.forEach(cat => { %>
      <li>
        <a href="/categories/<%= cat.id %>"> <%= cat.category_name %> </a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
